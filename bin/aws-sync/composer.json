{
    "name": "wow-be/aws-sync",
    "description": "Synchronize Automatic Weather Stations (AWS) and Observations from Oracle to PostgreSQL WOW database.",
    "license": "proprietary",
    "type": "project",
    "version": "1.0.0",
    "require": {
        "php": "^8.3",
        "ext-pdo_oci": "*",
        "ext-pdo_pgsql": "*",
        "ramsey/uuid": "^4.9",
        "symfony/console": "^7.3",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "laravel/pint": "^1.24",
        "phpstan/phpstan": "^2.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AwsSync\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse -l 7 src",
        "pint": "pint --test",
        "pint:fix": "pint"
    }
}
